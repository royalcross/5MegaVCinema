<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
    <mapper namespace="com.itwillbs.vCinema.mapper.NoticeMapper">
    <insert id="insertNotice">
		INSERT
		INTO notice
		VALUES (
			null
			, #{notice_num}
			, #{notice_type}
			, #{notice_subject}
			, #{notice_content}
			,now()
<!-- 			, #{notice_date} -->
			  
		)
	</insert>

    
    </mapper>