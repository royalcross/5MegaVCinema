<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
    <mapper namespace="com.itwillbs.vCinema.mapper.CsMapper">
    <insert id="insertCs">
		INSERT
		INTO cs
		VALUES (
			 #{cs_num}
			, #{cs_member_num}
			, #{cs_category}
			, #{cs_subject}
			, #{cs_content}
			, #{cs_date}
			, #{cs_check}
			, #{cs_answer}
			, #{cs_phonenumber}
			, #{cs_theater_name}		  
		)
	</insert>
		
    
    </mapper>